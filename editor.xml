<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="13" />
  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="ADDITIONAL_PAGE_AT_BOTTOM" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="BACKGROUND" value="65686a" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="c04b10" />
      <option name="BACKGROUND" value="686b6d" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="die">
      <option name="FOREGROUND" value="c00d00" />
      <option name="BACKGROUND" value="626567" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bconsole.log\(.*\)\b">
      <option name="FOREGROUND" value="c00006" />
      <option name="BACKGROUND" value="797c7e" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\boptimise\b.*">
      <option name="FOREGROUND" value="2c003" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="2c003" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
      <option name="EFFECT_TYPE" value="4" />
    </pattern>
    <filter name="IMPORTANT">
      <pattern index="1" />
      <pattern index="3" />
      <pattern index="2" />
    </filter>
    <filter name="TODOs">
      <pattern index="0" />
      <pattern index="4" />
    </filter>
  </component>
</application>